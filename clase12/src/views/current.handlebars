<div id="result"></div>

<script>
    fetch('/jwt/current', {
        headers: {
            'Current-type': 'application/json',
            //'auth': `${localStorage.getItem('auth')}`
        }
    }).then(r => r.json())
        .then(data => {
            document.getElementById('result').innerHTML = JSON.stringify(data)
        })
</script>